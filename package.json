{
  "name": "y",
  "version": "1.0.0",
  "description": "A set of React UI components",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production --config webpack.prod.js",
    "build-demo": "npm install && cd doc && npm install && npm run build",
    "lint": "eslint ./ --ext .jsx --ext .js",
    "test": "nyc mocha --require babel-core/register --require jsdom-global/register --require test/testSetup.js --require test/ignoreStyles.js 'src/**/*.test.js'",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "postinstall": "gitmoji -i"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 20,
    "statements": 20,
    "functions": 20,
    "branches": 20,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.test.js"
    ]
  },
  "repository": "https://github.com/Codebrahma/React-Lite-UI.git",
  "license": "ISC",
  "private": false,
  "keywords": [
    "React",
    "Components"
  ],
  "author": "Codebrahma",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^2.0.0",
    "gitmoji-cli": "^1.9.2",
    "jsdom": "12.0.0",
    "jsdom-global": "3.0.2",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.8.3",
    "nyc": "^13.0.1",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "classnames": "^2.2.6",
    "html-webpack-plugin": "^3.2.0",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-css-themr": "^2.1.2",
    "react-dom": "^16.3.2",
    "react-live": "^1.11.0",
    "sinon": "^6.3.4"
  }
}
